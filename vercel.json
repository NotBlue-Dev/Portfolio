{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "cleanUrls": true,
    "headers": [
      {
        "source": "/fonts/DrukWideBold.ttf",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/fonts/Montserrat-Bold.ttf",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/fonts/Montserrat-Medium.ttf",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/fonts/Montserrat-Regular.ttf",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      },
      {
        "source": "/fonts/Montserrat-SemiBold.ttf",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "public, max-age=31536000, immutable"
          }
        ]
      }
    ],
    "redirects": [
      {
        "source": "/home",
        "destination": "/"
      },
      {
        "source": "/sitemap",
        "destination": "/sitemap.xml"
      },
      {
        "source": "/cv",
        "destination": "https://github.com/NotBlue-Dev/"
      },
      {
        "source": "/social",
        "destination": "https://discord.com/users/228134038001549312"
      }
    ]
  }